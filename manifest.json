{
    "manifest_version": 3,
    "name": "Youtube Cleanup",
    "description": "Does basic cleanup of some youtube features",
    "version": "1.0",
    "action": {
        "default_popup": "hello.html",
        "default_icon": "images/smiling_imp.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://www.youtube.com/results*"
            ]
        }
    ]
}